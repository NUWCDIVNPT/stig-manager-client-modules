        {
          "uuid": "4f074e96-8244-11ee-8b44-13c1c13d16bb",
          "stig_uuid": "4f06b250-8244-11ee-8b44-13c1c13d16bb",
          "target_key": null,
          "stig_ref": null,
          "group_id": "V-230227",
